<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.2" />
  </startup>
  <system.serviceModel>
    <bindings>
      <basicHttpBinding>
        <binding name="BasicHttpBinding_ICityService" />
        <binding name="BasicHttpBinding_IStreetService" />
        <binding name="BasicHttpBinding_IZoneService" />
        <binding name="BasicHttpBinding_IAgencyService" />
        <binding name="BasicHttpBinding_IUserService" />
        <binding name="BasicHttpBinding_ILoginService" />
        <binding name="BasicHttpBinding_IRouteService" />
        <binding name="BasicHttpBinding_IStopService" />
        <binding name="BasicHttpBinding_IFareService" />
        <binding name="BasicHttpBinding_ISearchService" />
      </basicHttpBinding>
    </bindings>
    <client>
      <endpoint address="http://localhost:8733/cities" binding="basicHttpBinding"
        bindingConfiguration="BasicHttpBinding_ICityService" contract="Services.Cities.ICityService"
        name="BasicHttpBinding_ICityService" />
      <endpoint address="http://localhost:8733/streets" binding="basicHttpBinding"
        bindingConfiguration="BasicHttpBinding_IStreetService" contract="Services.Streets.IStreetService"
        name="BasicHttpBinding_IStreetService" />
      <endpoint address="http://localhost:8733/zones" binding="basicHttpBinding"
        bindingConfiguration="BasicHttpBinding_IZoneService" contract="Services.Zones.IZoneService"
        name="BasicHttpBinding_IZoneService" />
      <endpoint address="http://localhost:8733/agencies" binding="basicHttpBinding"
        bindingConfiguration="BasicHttpBinding_IAgencyService" contract="Services.Agencies.IAgencyService"
        name="BasicHttpBinding_IAgencyService" />
      <endpoint address="http://localhost:8733/users" binding="basicHttpBinding"
        bindingConfiguration="BasicHttpBinding_IUserService" contract="Services.Users.IUserService"
        name="BasicHttpBinding_IUserService" />
      <endpoint address="http://localhost:8733/login" binding="basicHttpBinding"
        bindingConfiguration="BasicHttpBinding_ILoginService" contract="Services.Login.ILoginService"
        name="BasicHttpBinding_ILoginService" />
      <endpoint address="http://localhost:8733/routes" binding="basicHttpBinding"
        bindingConfiguration="BasicHttpBinding_IRouteService" contract="Services.Routes.IRouteService"
        name="BasicHttpBinding_IRouteService" />
      <endpoint address="http://localhost:8733/stops" binding="basicHttpBinding"
        bindingConfiguration="BasicHttpBinding_IStopService" contract="Services.Stops.IStopService"
        name="BasicHttpBinding_IStopService" />
      <endpoint address="http://localhost:8733/search" binding="basicHttpBinding"
        bindingConfiguration="BasicHttpBinding_ISearchService" contract="Services.Search.ISearchService"
        name="BasicHttpBinding_ISearchService" />
      <endpoint address="http://localhost:8733/fares" binding="basicHttpBinding"
        bindingConfiguration="BasicHttpBinding_IFareService" contract="Services.Fares.IFareService"
        name="BasicHttpBinding_IFareService" />
    </client>
  </system.serviceModel>
</configuration>